    "laravel/sanctum": "^4.0",
    "laravel/tinker": "^2.8",
    "maatwebsite/excel": "^3.1",
    "guzzlehttp/guzzle": "^7.0",
    "phpoffice/phpexcel": "^1.8"
  },
  "require-dev": {
